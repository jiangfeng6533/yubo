<!--pages/User/user/user.wxml-->
<view class='UCenter-bg'>
    <image src='/images/logo.png' class='png' mode='widthFix'></image>
    <view class='text-xl'> Cola </view>
    <view class='margin-top-sm'>
        <text>普通管理员</text>
    </view>
    <image src='https://ws3.sinaimg.cn/large/005BYqpgly1g1hovoxxodg30f002skhn.jpg' mode='scaleToFill' class='gif-wave'></image>
</view>
<view class='padding flex text-center text-grey bg-white shadow-warp'>
    <view class='flex flex-sub flex-direction solid-right'>
        <view class="text-xxl text-orange">{{visitTotal}}</view>
        <view class="margin-top-sm">
            <text class='icon-discoverfill'></text> 维修数量</view>
    </view>
    <view class='flex flex-sub flex-direction solid-right'>
        <view class="text-xxl text-blue">{{starCount}}</view>
        <view class="margin-top-sm">
            <text class='icon-moneybag'></text> 收益</view>
    </view>
    <view class='flex flex-sub flex-direction'>
        <view class="text-xxl text-green">{{forksCount}}</view>
        <view class="margin-top-sm">
            <text class='icon-sponsor'></text> 余额</view>
    </view>
</view>
<view class="cu-list menu card-menu margin-top-xl margin-bottom-xl shadow-lg">
    <view class="cu-item arrow">
        <view class="content" bindtap="toServiceOrderList">
            <text class='icon-send text-cyan'></text>
            <text class='text-grey'>我的维修</text>
        </view>
    </view>
    <view class="cu-item arrow">
        <navigator class='content' open-type="navigate" hover-class='none' url="/pages/vipcard/index/index">
            <text class='icon-vipcard text-green'></text>
            <text class='text-grey'>货品订单</text>
        </navigator>
    </view>
    <!-- <view class="cu-item arrow">
        <navigator class='content' open-type="navigate" hover-class='none' url="/pages/user/address/index">
            <text class='icon-location text-orange'></text>
            <text class='text-grey'>地址管理</text>
        </navigator>
    </view> -->
    <view class="cu-item arrow">
        <button class='cu-btn content' open-type='feedback'>
            <text class='icon-writefill text-grey'></text>
            <text class='text-grey'>意见反馈</text>
        </button>
    </view>
    <view class="cu-item arrow">
        <button class='cu-btn content'  bindtap="logout">
            <text class='cuIcon-exit text-grey newcon'> </text>
            <text class='text-grey'> 退出登录</text>
        </button>
    </view>
</view>

<view class='ub ub-f1' style="height:230rpx;">
</view>


